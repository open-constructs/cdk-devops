  "devDependencies": {
    "@stylistic/eslint-plugin": "^2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "aws-cdk-lib": "2.222.0",
    "commit-and-tag-version": "^12",
    "constructs": "10.4.2",
    "eslint": "^9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.2.0",
    "jest-junit": "^16",
    "jsii": "~5.9.0",
    "jsii-diff": "^1.119.0",
    "jsii-docgen": "^10.5.0",
    "jsii-pacmak": "^1.119.0",
    "jsii-rosetta": "~5.9.0",
    "projen": "^0.98.11",
    "projen-pipelines": "^0.3.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
